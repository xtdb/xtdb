query ITTTT nosort
SELECT P2.property_number,
       GREATEST(P1.xt$valid_from, P2.xt$valid_from) AS VT_Begin,
       LEAST(P1.xt$valid_to, P2.xt$valid_to) AS VT_End,
       GREATEST(P1.xt$system_from, P2.xt$system_from) AS Recorded_Start,
