[:project
 [{revenue xt$sum_out_2}]
 [:group-by
  [{xt$sum_out_2 (sum xt$sum_in_3)}]
  [:map
   [{xt$sum_in_3 (* l.1/l_extendedprice l.1/l_discount)}]
   [:rename
    l.1
    [:scan
     {:table lineitem}
     [{l_shipdate
       (and
        (<
         l_shipdate
         (+
          #time/date "1994-01-01"
          (single-field-interval "1" "YEAR" 2 6)))
        (>= l_shipdate #time/date "1994-01-01"))}
      {l_quantity (< l_quantity 24)}
      {l_discount (between l_discount 0.05 0.07)}
      l_extendedprice]]]]]]
