[:project
 {:projections [{movie_title si.1/movie_title}]}
 [:mega-join
  {:conditions [{si.1/star_name ms.2/name}]}
  [[:rename
    si.1
    [:scan {:table #xt/table stars_in, :columns [star_name movie_title]}]]
   [:rename
    ms.2
    [:scan
     {:table #xt/table movie_star, :columns [{birthdate (and (< birthdate 1960) (> birthdate 1950))} name]}]]]]]
