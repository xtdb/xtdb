[:project
 [{name s.1/name} {course e.2/course}]
 [:select
  (>= e.2/grade _sq_3)
  [:map
   [{_sq_3 (+ _avg_out_8 1)}]
   [:group-by
    [s.1/name
     s.1/year
     s.1/major
     s.1/id
     e.2/sid
     e.2/grade
     e.2/course
     _row_number_0
     {_avg_out_8 (avg e2.4/grade)}]
    [:left-outer-join
     [(or
       (= s.1/id e2.4/sid)
       (and (= e2.4/curriculum s.1/major) (> s.1/year e2.4/date)))]
     [:map
      [{_row_number_0 (row-number)}]
      [:mega-join
       [{s.1/id e.2/sid}]
       [[:rename
         s.1
         [:scan
          {:table public/students}
          [name
           year
           {major (or (= major "CS") (= major "Games Eng"))}
           id]]]
        [:rename
         e.2
         [:scan {:table public/exams} [sid grade course]]]]]]
     [:rename
      e2.4
      [:scan {:table public/exams} [sid date curriculum grade]]]]]]]]
