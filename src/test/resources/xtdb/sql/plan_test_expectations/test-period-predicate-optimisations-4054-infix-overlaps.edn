[:project
 {:projections [{name foo.1/name}]}
 [:rename
  {:prefix foo.1}
  [:project
   {:projections
    [name
     _valid_from
     _valid_to
     {_valid_time (period _valid_from _valid_to)}]}
   [:scan
    {:table #xt/table foo,
     :columns
     [name
      {_valid_from
       (<
        _valid_from
        (coalesce #xt/zdt "2001-01-01T00:00Z" xtdb/end-of-time))}
      {_valid_to
       (>
        (coalesce _valid_to xtdb/end-of-time)
        #xt/zdt "2000-01-01T00:00Z")}]}]]]]
